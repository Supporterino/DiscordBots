{
  "name": "discordbots",
  "version": "0.1.0",
  "description": "Project for multiple discord bots",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node build/index.js",
    "start:dev": "ts-node ./src/index.ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "docu": "rm -Rf public && npx typedoc --options 'typedoc.json'"
  },
  "author": "Supporterino",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Supporterino/DiscordBots.git"
  },
  "dependencies": {
    "@discordjs/builders": "^0.1.1",
    "@pm2/io": "^5.0.0",
    "bufferutil": "^4.0.3",
    "discord.js": "github:discordjs/discord.js",
    "erlpack": "github:discord/erlpack",
    "nanoid": "^3.1.23",
    "tslog": "^3.2.0",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^14.17.6",
    "dotenv": "^10.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typescript": "^4.3.5"
  }
}
